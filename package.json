{
  "name": "storybook-lab",
  "version": "0.0.8",
  "description": "A project to learn React Storybook",
  "main": "./dist/index.js",
  "scripts": {
    "storybook": "start-storybook -s ./.storybook-assets -p 9001 ",
    "storybook:build": "build-storybook -s ./.storybook-assets -o docs",
    "storybook:deploy": "storybook-to-ghpages",
    "build-storybook": "build-storybook -s ./.storybook-assets -o .out",
    "build": "npm run lint && NODE_ENV=production babel src --out-dir dist",
    "prepublish": "npm run build",
    "lint": "./node_modules/.bin/eslint src/ --ext .jsx,.js",
    "lint:fix": "./node_modules/.bin/eslint src/ --ext .jsx,.js --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "storybook-deployer": {
    "gitUsername": "Estagi√°rio Juninho",
    "gitEmail": "juninho@bot.com",
    "commitMessage": "Deploy Storybook [skip ci]"
  },
  "author": "Willian Ribeiro <willianribeiro.contato@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "node-sass": "^4.5.3",
    "css-loader": "^0.28.2",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.0"
  },
  "dependencies": {
    "@kadira/storybook-deployer": "^1.2.0",
    "classnames": "^2.2.5",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willianribeiro/storybook-lab.git"
  },
